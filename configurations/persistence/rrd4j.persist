	Lastmotion_FF_MBR : strategy = everyMinute, restoreOnStartup
	Lastmotion_SF_Hall : strategy = everyMinute, restoreOnStartup
	Heat_AS_Duty_Cycle : strategy = everyMinute, restoreOnStartup
	Heat_FF_Duty_Cycle : strategy = everyMinute, restoreOnStartup
	Heat_SF_Duty_Cycle : strategy = everyMinute, restoreOnStartup
	Cool_AS_Duty_Cycle : strategy = everyMinute, restoreOnStartup
	Cool_FF_Duty_Cycle : strategy = everyMinute, restoreOnStartup
	Cool_SF_Duty_Cycle : strategy = everyMinute, restoreOnStartup
	Lastmotion_AS_Studio : strategy = everyChange, restoreOnStartup
	Attic_Uptime : strategy = everyMinute, restoreOnStartup
	Attic_lsp : strategy = everyMinute, restoreOnStartup
	Attic_wifi_strength : strategy = everyMinute, restoreOnStartup
